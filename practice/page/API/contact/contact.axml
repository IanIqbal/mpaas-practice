<view class="page">

  <view class="page-description">contacts API</view>

  <view class="page-section">
    <view class="page-section-title">my.choosePhoneContact</view>
    <view class="page-section-demo">
      <button type="primary" onTap="choosePhoneContact">Evoke local address book</button>
    </view>
  </view>

